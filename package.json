{
  "name": "work-orders",
  "version": "0.6.0",
  "private": true,
  "description": "PlumbTix \u2014 Work Order Management by Pro Roto Inc.",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint src/ --ext .ts,.tsx",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:verify": "supabase db reset && echo '=== All migrations applied ==='",
    "supabase:diff": "supabase db diff --use-migra",
    "functions:serve": "supabase functions serve --env-file ./supabase/.env.local",
    "functions:deploy": "supabase functions deploy"
  },
  "dependencies": {
